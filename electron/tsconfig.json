/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "extends":"../tsconfig.json",
  "compilerOptions": {
    "outDir": "./../dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "commonjs",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
